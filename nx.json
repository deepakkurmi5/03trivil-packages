{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "test"
        ]
      }
    }
  },
  "namedInputs": {
    "all": [
      "{projectRoot}/**/*"
    ],
    "public": [
      "!{projectRoot}/**/*.test.*"
    ]
  },
  "targetDefaults": {
    "test": {
      "inputs": [
        "all",
        "^public"
      ]
    }
  }
}